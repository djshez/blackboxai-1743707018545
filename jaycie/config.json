{
    "googleApiEndpoint": "https://api.google.com/v1/ai-platform",
    "projectId": "jaycie-ai-project",
    "botMode": "chat",
    "server": {
        "host": "0.0.0.0",
        "port": 8000,
        "debug": false
    },
    "ai": {
        "modelVersion": "1.0.0",
        "learningRate": 0.01,
        "maxTokens": 1000,
        "temperature": 0.7,
        "contextWindow": 10
    },
    "frontend": {
        "maxMessageLength": 500,
        "messageHistoryLimit": 100,
        "typingIndicatorDelay": 1500,
        "refreshInterval": 5000
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "file": "logs/jaycie.log"
    },
    "security": {
        "corsEnabled": true,
        "allowedOrigins": ["http://localhost:8000"],
        "rateLimit": {
            "maxRequests": 100,
            "perMinute": 60
        }
    }
}